{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "./asm6 mult4packed.s mult4packed.bin",
            "problemMatcher": {
                "owner": "asm",
                "fileLocation": ["relative", "${workspaceFolder}"],
                "pattern": {
                    "regexp": "^(.*)\\(([0-9]*)\\):\\s(.*)",
                    "file": 1,
                    "line": 2,
                    "message": 3
                }
            }
        }
    ]
}